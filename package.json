{
  "name": "bun-onchain",
  "module": "src/serve.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/serve.ts",
    "build": "bun build --compile --asset-naming=[name].[ext] --minify --sourcemap --target=bun-windows-x64 ./src/serve.ts --outfile cad-serve",
    "serve": "bun src/serve.ts",
    "docker": "yarn build && docker build --platform linux/amd64 -t bun ."
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/bun": "latest",
    "bun-types": "latest",
    "bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@std/media-types": "npm:@jsr/std__media-types",
    "adm-zip": "^0.5.16",
    "onchain-pulsar": "0.0.3",
    "onchain-sdk": "3.9.4",
    "@pdfme/generator": "1.0.17",
    "pdf-lib": "^1.17.1",
    "pdf-merger-js": "^4.1.2"
  }
}
